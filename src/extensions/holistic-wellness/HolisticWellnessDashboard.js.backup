      osc.connect(ctx.destination);
      osc.start();
      setAudioCtx(ctx);
      setOscillator(osc);
      setIsPlaying(true);
      const id = setTimeout(() => {
        stopFrequency();
      }, duration * 1000);
      setTimerId(id);
    }
  };

